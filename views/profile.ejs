    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Profile</title>
    <link rel="stylesheet" href="css/styles3.css">
    <link rel="stylesheet" href="css/styles.css">
    <style>

      button{
        width : 200px;
        height : 50px;
        font-size : 1.4rem;
        border-radius : 10px;
        background-color : blue;
        color : white;
        margin-bottom : 40px;
      }




      .warning{
        background-color: white;
        color: red;
        border : 4px solid black;
        margin-bottom : 67px;
      }

      .Prof{
        background-color: yellow;
        border-radius : 15px;
        border : 2px solid black;
        width: 50%;
        margin-left : 25%;
        margin-bottom : 67px;
      }

      @media only screen and (max-width : 502px) {
        .Prof{
          width : 100%;
          margin-left : 0;
        }
      }
    </style>
  </head>
  <body>
    <div class="Prof">
      <h1>Profile</h1>
      <h3>Firt Name : <%= Fname %></h3>
      <h3>Second Name : <%= Sname %></h3>
      <h3>Email : <%= email %></h3>
      <h3>Contact No : <%= no %></h3>
      <h3>Password : <span styel = "type = "password"" type = "password"><%= pwsd %></span></h3>
    </div>


    <div class="warning">
        <p class="warnTitle">Warning : Sorry, the above cannot be edited!!!.</p>
    </div>

    <form  action="/dashboard2" method="post">
